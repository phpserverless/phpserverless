{
    "name": "phpserverless\/phpserverless",
    "description": "The Serverless PHP Framework",
    "homepage": "https:\/\/github.com\/phpserverless\/phpserverless",
    "license": "proprietary",
    "authors": [
        {
            "name": "Milan Lesichkov",
            "email": "lesichkovm@gmail.com"
        }
    ],
    "autoload": {
        "files": [
            "env.php",
            "main.php"
        ],
        "psr-4": {
            "App\\": "app\/",
            "Tests\\": "tests\/"
        }
    },
    "require": {
        "dg\/composer-cleaner": "v2.1",
        "jenssegers\/blade": "^1.3",
        "mrclay\/jsmin-php": "^2.4",
        "phroute\/phroute": "^2.1",
        "sinevia\/php-library-api": "^1.1",
        "sinevia\/php-library-serverless": "^1.7",
        "sinevia\/php-library-sqldb": "^3.8",
        "sinevia\/php-library-utils": "^3.3"
    },
    "require-dev": {
        "lesichkovm\/phpsls": "^1.8",
        "phpunit\/phpunit": "^8.5",
        "sinevia\/php-library-migrate": "^1.4"
    },
    "extra": {
        "cleaner-ignore": {
            "phpunit\/phpunit": true,
            "vlucas\/valitron": true
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "vendor\/bin\/phpunit"
    }
}